<?xml version="1.0" encoding="UTF-8" ?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1"
         metadata-complete="false">

   <display-name>AutoparkWeb</display-name>

    <welcome-file-list>
        <welcome-file>resources/index.html</welcome-file>
    </welcome-file-list>

    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>/resources/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewCarTypesServlet</servlet-name>
        <servlet-class>by.incubator.application.servlets.ViewCarTypesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewCarTypesServlet</servlet-name>
        <url-pattern>/viewTypes</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewCarsServlet</servlet-name>
        <servlet-class>by.incubator.application.servlets.ViewCarsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewCarsServlet</servlet-name>
        <url-pattern>/viewCars</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewInfoServlet</servlet-name>
        <servlet-class>by.incubator.application.servlets.ViewInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewInfoServlet</servlet-name>
        <url-pattern>/viewInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewReportServlet</servlet-name>
        <servlet-class>by.incubator.application.servlets.ViewReportServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewReportServlet</servlet-name>
        <url-pattern>/viewReport</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewDiagnosticServlet</servlet-name>
        <servlet-class>by.incubator.application.servlets.ViewDiagnosticServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewDiagnosticServlet</servlet-name>
        <url-pattern>/viewDiagnostic</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewPlannedDiagnosticServlet</servlet-name>
        <servlet-class>by.incubator.application.servlets.ViewPlannedDiagnosticServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewPlannedDiagnosticServlet</servlet-name>
        <url-pattern>/viewPlannedDiagnostic</url-pattern>
    </servlet-mapping>
</web-app>